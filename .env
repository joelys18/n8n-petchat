# Puerto donde corre n8n (Render lo ignorará, pero útil localmente)
N8N_PORT=5678

# URL pública (Render generará una automáticamente, luego la actualizas)
WEBHOOK_URL=https://tu-url-de-render.onrender.com

# Base de datos (usarás la de Laravel o una independiente)
DB_TYPE=postgresdb
DB_POSTGRESDB_HOST=tu_host_de_db
DB_POSTGRESDB_PORT=5432
DB_POSTGRESDB_DATABASE=nombre_de_tu_db
DB_POSTGRESDB_USER=tu_usuario
DB_POSTGRESDB_PASSWORD=tu_password

# Cifrado interno
N8N_ENCRYPTION_KEY=una_clave_larga_y_segura

# Modo de ejecución
GENERIC_TIMEZONE=America/Mexico_City
EXECUTIONS_MODE=regular

# Deshabilita el registro de nuevas cuentas en el panel n8n
N8N_USER_MANAGEMENT_DISABLED=false
